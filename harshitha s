#include <stdio.h>
// sum of two numbers
int main() {
    float num1, num2, sum;
    printf("Enter first number: ");
    scanf("%f", &num1);
    printf("Enter second number: ");
    scanf("%f", &num2);
    sum = num1 + num2;
    printf("The sum is: %.2f\n", sum);
    return 0;
}
